<Application x:Class="MalifoApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dialog="clr-namespace:MalifoApp.Dialogs"
             xmlns:vm="clr-namespace:MalifoApp.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <dialog:EditDeckDialogBox x:Key="{x:Type vm:EditDeckDialogViewModel}" x:Shared="False" />
        <dialog:OpenFileDialogPresenter x:Key="{x:Type vm:OpenFileDialogViewModel}" />
    </Application.Resources>
</Application>
